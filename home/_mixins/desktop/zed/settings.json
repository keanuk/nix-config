{
  "auto_install_extensions": {
    "asciidoc": true,
    "basher": true,
    "cargo-tom": true,
    "catppuccin": true,
    "catppuccin-blur": true,
    "catppuccin-icons": true,
    "csv": true,
    "dart": true,
    "docker-compose": true,
    "dockerfile": true,
    "env": true,
    "git-firefly": true,
    "golangci-lint": true,
    "gosum": true,
    "graphql": true,
    "haskell": true,
    "helm": true,
    "html": true,
    "http": true,
    "java": true,
    "just": true,
    "kotlin": true,
    "log": true,
    "lua": true,
    "make": true,
    "nix": true,
    "nu": true,
    "proto": true,
    "pylsp": true,
    "scss": true,
    "sql": true,
    "svelte": true,
    "swift": true,
    "terraform": true,
    "toml": true,
    "typst": true,
    "vue": true,
    "xml": true,
    "zig": true
  },
  "autosave": "on_focus_change",
  "buffer_font_size": 14,
  "features": {
    "edit_prediction_provider": "copilot"
  },
  "icon_theme": "Catppuccin Mocha",
  "languages": {
    "Nix": {
      "formatter": {
        "external": {
          "command": "alejandra"
        }
      },
      "language_servers": ["nil", "!nixd"]
    }
  },
  "lsp": {
    "nil": {
      "initialization_options": {
        "formatting": {
          "command": ["alejandra"]
        }
      }
    }
  },
  "restore_on_startup": "last_session",
  "tab_size": 2,
  "terminal": {
    "button": true,
    "dock": "bottom",
    "font_family": "RobotoMono Nerd Font",
    "font_features": null,
    "font_size": null,
    "line_height": "standard",
    "shell": {
      "program": "fish"
    },
    "toolbar": {
      "breadcrumbs": true
    },
    "working_directory": "current_project_directory"
  },
  "theme": {
    "dark": "Catppuccin Mocha (Blur)",
    "light": "Catppuccin Latte (Blur)",
    "mode": "system"
  },
  "ui_font_size": 14,
  "helix_mode": true,
  "vim": {
    "default_mode": "normal",
    "toggle_relative_line_numbers": true
  },
  "agent": {
    "default_model": {
      "model": "claude-opus-4.5",
      "provider": "copilot_chat"
    },
    "enabled": true,
    "button": true
  }
}
